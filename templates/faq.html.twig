{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('css/faq') }}
{% endblock %}
{% block title %}{% endblock %}


{% block body %}
    <div class="section no-pad-bot">
        <div class="row">
            <div class="col s12">
                <ul class="collapsible">
                    {% for section in sections %}
                        <li>
                            <div class="collapsible-header">{{ section.title }}</div>
                            <div class="collapsible-body"><span>{{ section.content | raw }}</span></div>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('js/faq') }}
{% endblock %}
